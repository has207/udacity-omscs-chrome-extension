{
  "name": "Udacity OMSCS Chrome extension",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Enable signing in to Georgia Tech from the Udacity signin page, and force logout when GATech credentials expire.",
  "icons": {"48": "icon48.png", "128": "icon128.png"},
  "permissions": [
      "*://www.udacity.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.udacity.com/course/viewer"],
      "js": ["omscs.js"]
    },
    {
      "matches": ["*://www.udacity.com/georgia-tech/welcome"],
      "js": ["link.js"]
    },
    {
      "matches": ["*://www.udacity.com/account/auth"],
      "js": ["signin.js"]
    }
  ]
}
