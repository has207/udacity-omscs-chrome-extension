{
  "name": "Udacity OMSCS Chrome extension",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Detect when Georgia Tech has signed you out of Udacity.",
  "icons": {"48": "icon48.png", "128": "icon128.png"},
  "permissions": [
      "*://www.udacity.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.udacity.com/*"],
      "exclude_matches": ["*://www.udacity.com/georgia-tech/welcome"],
      "js": ["omscs.js"]
    },
    {
      "matches": ["*://www.udacity.com/georgia-tech/welcome"],
      "js": ["link.js"]
    },
    {
      "matches": ["*://www.udacity.com/account/auth"],
      "js": ["signin.js"]
    }
  ]
}
